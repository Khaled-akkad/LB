	{
		"title": "Läkemedel för antikoagulationsbehandling",
		"level": 2,
		"id": "Lakemedel-for-antikoagulationsbehandling",
		"parentId": "Venos-tromboembolism",
		"chapter": "",
		"url": "/kapitel/blod/Ven-s-tromboembolism.html",
		"type": "header",
