		{
			"id": "d194acec-11b5-4d88-9bf5-cd73917f0198",
			"name": "d194acec-11b5-4d88-9bf5-cd73917f0198",
			"type": "text",
			"content": "<h1 id=\"p1_4\">Atopi, allergi och &ouml;verk&auml;nslighet</h1>",
			"settings": {
				"preprocessors": {
					"fixlinkstoself.js": "false",
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
