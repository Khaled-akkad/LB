		{
			"id": "eee9e0d2-b32d-49e0-8366-8a36ade9516d",
			"name": "eee9e0d2-b32d-49e0-8366-8a36ade9516d",
			"type": "text",
			"content": "<h2 id=\"b3_23\">Gallbl&aring;se- och gallg&aring;ngscancer</h2>",
			"settings": {
				"preprocessors": {
					"idinjection.js": "false",
					"fixlinkstoself.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
