				"number": "1",
				"title": "Kognitiva effekter av antiepileptiska läkemedel",
				"id": "",
				"text": "<table class=\"table table-bordered\">\r\n<tbody>\r\n<tr>\r\n<td><strong>Liten eller ingen</strong></td>\r\n<td><strong>Viss</strong></td>\r\n<td><strong>Potentiellt signifikant</strong></td>\r\n</tr>\r\n<tr>\r\n<td>Gabapentin</td>\r\n<td>Fenytoin</td>\r\n<td>Fenobarbital</td>\r\n</tr>\r\n<tr>\r\n<td>Lamotrigin</td>\r\n<td>Karbamazepin</td>\r\n<td>Topiramat</td>\r\n</tr>\r\n<tr>\r\n<td>Oxkarbazepin</td>\r\n<td>Valproat</td>\r\n<td></td>\r\n</tr>\r\n<tr>\r\n<td>Levetiracetam</td>\r\n<td>Zonisamid</td>\r\n<td></td>\r\n</tr>\r\n</tbody>\r\n</table>\r\n<p></p>"
