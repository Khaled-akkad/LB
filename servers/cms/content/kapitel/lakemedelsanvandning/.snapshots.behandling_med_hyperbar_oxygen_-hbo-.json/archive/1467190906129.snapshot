			"type": "text",
			"content": "<h1 id=\"v1_2\">Behandling med hyperbar oxygen (HBO)</h1>\n<h2 id=\"v1_3\"></h2>",
			"settings": {
				"preprocessors": {
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
