		{
			"id": "b0f8e2a4-9623-4eeb-a50d-5da37ddfee33",
			"name": "b0f8e2a4-9623-4eeb-a50d-5da37ddfee33",
			"type": "pagefootnote",
			"content": {
				"number": "1",
				"text": "<span class=\"tabstop\"></span>Sedan 2011 finns inga godkända läkemedel innehållande rosiglitazon i Sverige."
			},
			"settings": {
				"preprocessors": {
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
					"genericas.js": "false",
