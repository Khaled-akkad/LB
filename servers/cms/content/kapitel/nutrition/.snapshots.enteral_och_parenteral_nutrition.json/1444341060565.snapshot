		{
			"id": "7e7ed0c2-93c0-46dc-8b58-c48aa1827cb2",
			"name": "7e7ed0c2-93c0-46dc-8b58-c48aa1827cb2",
			"type": "facts",
			"content": {
				"number": "6",
				"title": "<a class=\"crossref\" id=\"90790\"></a>Orsaker till diarré vid enteral nutrition ",
				"text": "<table><tbody><tr><td><ul><li><span class=\"tab\"></span>Antibiotikabehandling</li></ul><ul><li><span class=\"tab\"></span>För hög belastning<br>– för snabb tillförsel<br>– hyperosmolära lösningar</li></ul><ul><li><span class=\"tab\"></span>Bakteriekontamination</li></ul></td></tr></tbody></table>",
				"id": "c4_21"
			},
			"settings": {
				"preprocessors": {
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
