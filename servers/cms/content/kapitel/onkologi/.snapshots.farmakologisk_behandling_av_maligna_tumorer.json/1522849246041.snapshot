				"text": "<table>\r\n<tbody>\r\n<tr>\r\n<td></td>\r\n<td style=\"text-align: center;\"><strong>Bleomycin</strong></td>\r\n<td style=\"text-align: center;\"><strong>Trabektedin</strong></td>\r\n</tr>\r\n<tr>\r\n<td>Alopeci</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n<td style=\"text-align: center;\">+</td>\r\n</tr>\r\n<tr>\r\n<td>Allergi</td>\r\n<td style=\"text-align: center;\">+</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n</tr>\r\n<tr>\r\n<td>Fatigue</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n<td style=\"text-align: center;\">+</td>\r\n</tr>\r\n<tr>\r\n<td>Benm&auml;rgsh&auml;mning</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n<td style=\"text-align: center;\">+</td>\r\n</tr>\r\n<tr>\r\n<td>Illam&aring;ende</td>\r\n<td style=\"text-align: center;\">+</td>\r\n<td style=\"text-align: center;\">+</td>\r\n</tr>\r\n<tr>\r\n<td>Diarr&eacute;</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n<td style=\"text-align: center;\">+</td>\r\n</tr>\r\n<tr>\r\n<td>F&ouml;rstoppning</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n<td style=\"text-align: center;\">+</td>\r\n</tr>\r\n<tr>\r\n<td>Mukosit</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n<td style=\"text-align: center;\">+</td>\r\n</tr>\r\n<tr>\r\n<td>Hj&auml;rttoxicitet</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n</tr>\r\n<tr>\r\n<td>Hudtoxicitet</td>\r\n<td style=\"text-align: center;\">+</td>\r\n<td style=\"text-align: center;\">+</td>\r\n</tr>\r\n<tr>\r\n<td>Infertilitet</td>\r\n<td style=\"text-align: center;\">+</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n</tr>\r\n<tr>\r\n<td>Levertoxicitet</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n<td style=\"text-align: center;\">+</td>\r\n</tr>\r\n<tr>\r\n<td>Lungtoxicitet</td>\r\n<td style=\"text-align: center;\">++</td>\r\n<td style=\"text-align: center;\">&ndash;</td>\r\n</tr>\r\n<tr>\r\n<td>Neurolo