d>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">21.15</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">2,0</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">+</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">++</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">00.30</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;1,5</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">23.05</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">1,0</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">++</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td colspan=\"4\">\r\n<p>Dygnsvolym 19 dl</p>\r\n<p>Frekvens 7 g&aring;nger/1 dygn</p>\r\n<p>Medelvolym 19/7 = 2,7 dl</p>\r\n<p>H&ouml;gsta volym 5,0 dl</p>\r\n</td>\r\n<td colspan=\"4\">\r\n<p>Dygnsvolym 15,25 dl</p>\r\n<p>Frekvens 11 g&aring;nger/1 dygn</p>\r\n<p>Medelvolym 15,25/11 = 1,4 dl</p>\r\n<p>H&ouml;gsta volym 2,5 dl</p>\r\n</td>\r\n<td colspan=\"4\">\r\n<p>Dygnsvolym 36,25 dl</p>\r\n<p>Frekvens 10 g&aring;nger/1 dygn</p>\r\n<p>Medelvolym 36,25/10 = 3,6 dl</p>\r\n<p>H&ouml;gsta volym 4,5 dl</p>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td colspan=\"12\" class=\"footNotes\">\r\n<div class=\"tableFootnote\" id=\"fnote_24\">a.\r\n<div class=\"tableLine\">Droppar (+), fuktig (++), v&aring;t (+++)</div>\r\n</div>\r\n<div class=\"tableFootnote\" id=\"fnote_2