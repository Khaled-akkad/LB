n<p><span style=\"color: #ff0000;\">+</span></p>\r\n<p><span style=\"color: #ff0000;\">+</span></p>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">\r\n<div class=\"tableLine\">?</div>\r\n<div class=\"tableLine\"><span style=\"color: #ff0000;\">+<br></span></div>\r\n<div class=\"tableLine\">?</div>\r\n<div class=\"tableLine\"><span style=\"color: #ff0000;\">+<br></span></div>\r\n<div class=\"tableLine\"></div>\r\n</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">IV<span class=\"tab\"></span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Verapamil</div>\r\n<div class=\"tableLine\">Diltiazem</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&uarr;</div>\r\n<div class=\"tableLine\">&uarr;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">(<span style=\"color: #ff0000;\">+</span>)</div>\r\n<div class=\"tableLine\">(<span style=\"color: #ff0000;\">+</span>)</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">(<span style=\"color: #ff0000;\">+</span>)</div>\r\n<div class=\"tableLine\">(<span style=\"color: #ff0000;\">+</span>)</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\"></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Digoxin</div>\r\n<div class=\"tableLine\">Digitoxin<span style=\"color: #ff0000;\"><sup>(</sup></span><span style=\"color: #ff0000;\"><sup>e)</sup></span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&uarr;</div>\r\n<div class=\"tableLine\">&uarr;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">(<span style=\"color: #ff0000;\">+</span>)</div>\r\n<div class=\"tableLine\">(<span style=\"color: #ff0000;\">+</span>)</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">&nbsp;</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine