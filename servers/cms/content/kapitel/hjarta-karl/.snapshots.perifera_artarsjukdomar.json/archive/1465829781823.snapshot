		{
			"id": "b758caf1-e2b4-41df-acdc-ff853d362970",
			"name": "b758caf1-e2b4-41df-acdc-ff853d362970",
			"type": "figure",
			"content": {
				"number": "2",
				"maxwidth": "",
				"image": "/images/e7_hja_periferaartar_2013fm10_images/figur2.png",
				"title": "",
				"text": "<p><a class=\"crossref\" id=\"93209\"></a>Fl&ouml;desschema &ndash; Behandling av <a class=\"index\" id=\"1026502\" data-text=\"kronisk ischemi\"></a>kronisk ischemi (modifierat efter referenser (1,2))</p>",
				"alt": "",
				"id": "e7_29"
			},
			"settings": {
