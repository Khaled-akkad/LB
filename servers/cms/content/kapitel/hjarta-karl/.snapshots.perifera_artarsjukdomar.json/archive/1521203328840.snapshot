			"type": "text",
			"content": "<h1 id=\"e7_3\">Perifera art&auml;rsjukdomar</h1>",
			"settings": {
				"preprocessors": {
					"idinjection.js": "false",
					"fixlinkstoself.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
