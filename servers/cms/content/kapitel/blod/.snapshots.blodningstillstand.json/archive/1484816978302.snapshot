			"content": "<h1 id=\"Blodningstillstand\">Bl&ouml;dningstillst&aring;nd</h1>",
			"settings": {
				"preprocessors": {
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
