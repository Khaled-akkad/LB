		{
			"id": "4846f1c2-db9a-4fc4-8394-e7e420a44ce0",
			"name": "4846f1c2-db9a-4fc4-8394-e7e420a44ce0",
			"type": "text",
			"content": "<h1 id=\"Koagulationsrubbningar\">Koagulationsrubbningar</h1>",
			"settings": {
				"preprocessors": {
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
					"genericas.js": "false",
