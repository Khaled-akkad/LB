			"type": "therapy",
			"content": {
				"number": "1",
				"title": "<a class=\"index\" id=\"18763\"></a>Tyreotoxikos",
				"id": "k3_10",
				"text": "<table>\r\n<tbody>\r\n<tr>\r\n<td><strong></strong></td>\r\n<td>Tyreostatika</td>\r\n<td>Operation</td>\r\n<td>Radiojod</td>\r\n</tr>\r\n<tr>\r\n<td><strong>Graves tyreotoxikos</strong></td>\r\n<td></td>\r\n<td></td>\r\n<td></td>\r\n</tr>\r\n<tr>\r\n<td>Barn, ungdomar (&lt; 20 &aring;r)</td>\r\n<td>+</td>\r\n<td>+</td>\r\n<td></td>\r\n</tr>\r\n<tr>\r\n<td>Vuxna (20&ndash;50 &aring;r)</td>\r\n<td></td>\r\n<td></td>\r\n<td></td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<ul>\r\n<li>ingen eller liten struma, m&aring;ttlig sjukdomsaktivitet</li>\r\n</ul>\r\n</td>\r\n<td>+</td>\r\n<td>+</td>\r\n<td>+</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<ul>\r\n<li>m&aring;ttlig struma, m&aring;ttlig sjukdomsaktivitet</li>\r\n</ul>\r\n</td>\r\n<td>+</td>\r\n<td>+</td>\r\n<td>+</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<ul>\r\n<li>stor struma, h&ouml;g sjukdomsaktivitet</li>\r\n</ul>\r\n</td>\r\n<td></td>\r\n<td>+</td>\r\n<td>+</td>\r\n</tr>\r\n<tr>\r\n<td>Vuxna (&gt; 50 &aring;r)</td>\r\n<td>+</td>\r\n<td></td>\r\n<td>+</td>\r\n</tr>\r\n<tr>\r\n<td>Recidiv efter tyreostatika</td>\r\n<td></td>\r\n<td>+</td>\r\n<td>+</td>\r\n</tr>\r\n<tr>\r\n<td>Recidiv efter operation</td>\r\n<td></td>\r\n<td></td>\r\n<td>+</td>\r\n</tr>\r\n<tr>\r\n<td><strong>Toxisk nod&ouml;s struma</strong></td>\r\n<td></td>\r\n<td>+ <a class=\"tableFootnoteLink\" href=\"#fnote_13\"><sup>a</sup></a></td>\r\n<td>+</td>\r\n</tr>\r\n<tr>\r\n<td><strong>Solit&auml;rt toxiskt adenom</strong></td>\r\n<td></td>\r\n<td>+ <sup>a</sup></td>\r\n<td>+</td>\r\n</tr>\r\n<tr>\r\n<td colspan=\"4\" class=\"footNotes\">\r\n<div class=\"tableFootnote\" id=\"fnote_13\"><strong>a.</strong> Operation &auml;r framf&ouml;r allt aktuell hos patienter &lt; 50 &aring;r och vid stor eller intratorakal struma.</div>\r\n</td>\r\n</tr>\r\n</tbody>\r\n</table>"
