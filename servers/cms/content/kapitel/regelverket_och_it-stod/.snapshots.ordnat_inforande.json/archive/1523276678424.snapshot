		{
			"id": "c64c60d1-61ca-436a-acca-bd8880dffbda",
			"name": "c64c60d1-61ca-436a-acca-bd8880dffbda",
			"type": "text",
			"content": "<h2 id=\"Samverkansmodellen\">Samverkansmodellen</h2>",
			"settings": {
				"preprocessors": {
					"fixlinkstoself.js": "false",
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
					"genericas.js": "false",
					"pagefootnotes.js": "false",
					"references.js": "false"
