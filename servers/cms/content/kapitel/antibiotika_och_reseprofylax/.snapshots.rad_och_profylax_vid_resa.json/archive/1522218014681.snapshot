		{
			"id": "86bf9417-04f9-40c6-bbfe-ca7a59614052",
			"name": "86bf9417-04f9-40c6-bbfe-ca7a59614052",
			"type": "tablenarrow",
			"content": {
				"number": "10",
				"title": "<a class=\"crossref\" id=\"11669\"></a>Doseringsschema för <a class=\"index\" id=\"1102406\" data-text=\"Malarone:doseringsschema\"></a>Malarone (atovakvon/proguanil)",
				"id": "l2_74",
				"text": "<table><tbody><tr><td><p><strong><div class=\"tableLine\">Kroppsvikt </div><div class=\"tableLine\">(kg)</div></strong></p></td><td><div class=\"tableLine\"><strong>Antal tabletter/dag</strong></div></td></tr><tr><td><div class=\"tableLine\">11–20</div></td><td><div class=\"tableLine\">1 Malarone Junior (62,5/25 mg)</div></td></tr><tr><td><div class=\"tableLine\">21–30</div></td><td><div class=\"tableLine\">2 Malarone Junior (62,5/25 mg)</div></td></tr><tr><td><div class=\"tableLine\">31–40</div></td><td><div class=\"tableLine\">3 Malarone Junior (62,5/25 mg)</div></td></tr><tr><td><div class=\"tableLine\">&nbsp;&nbsp;&nbsp;&gt;&nbsp;40</div></td><td><div class=\"tableLine\">Vuxna: 1 tablett Malarone 250/</div><div class=\"tableLine\">100 mg</div></td></tr></tbody></table>"
			},
