		{
			"id": "31f0e905-4609-48d4-b9e8-c12c35a7decc",
			"name": "31f0e905-4609-48d4-b9e8-c12c35a7decc",
			"type": "figure",
			"content": {
				"number": "2",
				"maxwidth": "",
				"image": "/images/l2_inf_reseprofylax_2013fm10_images/figur2.png",
				"title": "",
				"text": "<a class=\"crossref\" id=\"20939\"></a>Intyg för medicinsk kontraindikation mot gula febern-vaccination ",
				"alt": "",
				"id": "l2_45"
			},
			"settings": {
				"preprocessors": {
					"idinjection.js": "false",
					"fixlinkstoself.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
					"genericas.js": "false",
					"pagefootnotes.js": "false",
					"references.js": "false"
