		{
			"id": "8009a5c5-f005-4ce7-9354-5363de8e779a",
			"name": "8009a5c5-f005-4ce7-9354-5363de8e779a",
			"type": "tablenarrow",
			"content": {
				"number": "2",
				"title": "<a class=\"crossref\" id=\"87531\"></a>Vanlig etiologi vid akut  bakteriell meningit i olika åldersgrupper",
				"id": "a5_40",
				"text": "<table><tbody><tr><td><div class=\"tableLine\"><strong>Åldersgrupp<span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span></strong></div></td><td><div class=\"tableLine\"><strong>Typ av bakterie </strong></div></td></tr><tr><td><div class=\"tableLine\">Nyfödda<span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span> </div></td><td><div class=\"tableLine\">E. coli</div><div class=\"tableLine\">Grupp B-streptokocker</div><div class=\"tableLine\">Listeria</div></td></tr><tr><td><div class=\"tableLine\">1 månad–7 år<span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span> </div></td><td><div class=\"tableLine\">Pneumokocker</div><div class=\"tableLine\">Meningokocker </div><div class=\"tableLine\">H. influenzae typ B (hos </div><div class=\"tableLine\">ovaccinerade barn)</div></td></tr><tr><td><div class=\"tableLine\">7 år–30 år <span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span></div><div class=\"tableLine\"><span class=\"tab\"></span><span class=\"tab\"></span></div></td><td><div class=\"tableLine\">Meningokocker<br></div><div class=\"tableLine\">Pneumokocker</div></td></tr><tr><td><div class=\"tableLine\">30 år–70 år<span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span> <span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span></div></td><td><div class=\"tableLine\">Pneumokocker</div></td></tr><tr><td><div class=\"tableLine\">&gt;&nbsp;70 år<span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span><span class=\"tab\"></span></div></td><td><div class=\"tableLine\">Pneumokocker</div><div class=\"tableLine\">H. influenzae </div><div class=\"tableLine\">Listeria</div><div class=\"tableLine\">E. coli</div></td></tr></tbody></table>"
			},
			"settings": {
				"preprocessors": {
