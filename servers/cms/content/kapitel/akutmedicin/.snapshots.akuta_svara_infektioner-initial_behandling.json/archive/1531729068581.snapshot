			"content": "<h1 id=\"Gamla-kapitlet-\"><span style=\"background-color: #ffff00; color: #ff00ff;\">Gamla kapitlet &darr;</span></h1>\n<p>Text</p>",
			"settings": {
				"preprocessors": {
					"fixlinkstoself.js": "false",
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
