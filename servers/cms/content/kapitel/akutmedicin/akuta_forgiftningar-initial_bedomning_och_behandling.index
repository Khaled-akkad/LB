	{
		"title": "Antikolinergt syndrom",
		"level": 4,
		"id": "Antikolinergt-syndrom",
		"parentId": "Toxidrom",
		"chapter": "a6_aku_forgiftning_2013fm10.html",
		"url": "/kapitel/akutmedicin/akuta_forgiftningar-initial_bedomning_och_behandling.html",
		"type": "header",
