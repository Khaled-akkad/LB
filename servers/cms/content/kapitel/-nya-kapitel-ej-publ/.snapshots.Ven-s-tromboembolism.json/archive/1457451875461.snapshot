				"id": "",
				"text": "<table class=\"table table-bordered\">\r\n<tbody>\r\n<tr>\r\n<td></td>\r\n<td>Dabigatran</td>\r\n<td>Rivaroxaban</td>\r\n<td>Apixaban</td>\r\n<td>Edoxaban</td>\r\n<td>Warfarin</td>\r\n</tr>\r\n<tr>\r\n<td class=\"footNotes\">Prodrug</td>\r\n<td class=\"footNotes\">Ja</td>\r\n<td class=\"footNotes\">Nej</td>\r\n<td class=\"footNotes\">Nej</td>\r\n<td class=\"footNotes\">Nej</td>\r\n<td class=\"footNotes\">Nej</td>\r\n</tr>\r\n<tr>\r\n<td class=\"footNotes\">Mekanism</td>\r\n<td class=\"footNotes\">Direkt trombin-h&auml;mmare</td>\r\n<td class=\"footNotes\">Direkt faktor Xa-h&auml;mmare</td>\r\n<td class=\"footNotes\">Direkt faktor Xa-h&auml;mmare</td>\r\n<td class=\"footNotes\">Direkt faktor Xa-h&auml;mmare</td>\r\n<td class=\"footNotes\">H&auml;mmar syntes av vitamin K-beroende proteiner, faktor II, VII, IX och X men ocks&aring; protein C och protein S.</td>\r\n</tr>\r\n<tr>\r\n<td class=\"footNotes\">Dosering</td>\r\n<td class=\"footNotes\">Fast tv&aring;dos</td>\r\n<td class=\"footNotes\">Fast endos</td>\r\n<td class=\"footNotes\">Fast tv&aring;dos</td>\r\n<td class=\"footNotes\">Fast endos</td>\r\n<td class=\"footNotes\">Dosering efter INR, stor variation</td>\r\n</tr>\r\n<tr>\r\n<td class=\"footNotes\">Biotillg&auml;nglighet (%)</td>\r\n<td class=\"footNotes\">6</td>\r\n<td class=\"footNotes\">&gt; 80</td>\r\n<td class=\"footNotes\">&gt; 50</td>\r\n<td class=\"footNotes\">45</td>\r\n<td class=\"footNotes\">100</td>\r\n</tr>\r\n<tr>\r\n<td class=\"footNotes\">Tid till max plasma-koncentration (timmar)</td>\r\n<td class=\"footNotes\">2</td>\r\n<td class=\"footNotes\">3</td>\r\n<td class=\"footNotes\">3</td>\r\n<td class=\"footNotes\">1,5</td>\r\n<td class=\"footNotes\">120</td>\r\n</tr>\r\n<tr>\r\n<td class=\"footNotes\">T 1/2 (timmar)</td>\r\n<td class=\"footNotes\">80</td>\r\n<td class=\"footNotes\">33</td>\r\n<td class=\"footNotes\">25</td>\r\n<td class=\"footNotes\">35</td>\r\n<td class=\"footNotes\">1</td>\r\n</tr>\r\n<tr>\r\n<td class=\"footNote