		{
			"id": "c83bcabc-a696-4083-8bb3-6d7329653dd0",
			"name": "c83bcabc-a696-4083-8bb3-6d7329653dd0",
			"type": "facts",
			"content": {
				"number": "4",
				"title": "<a class=\"crossref\" id=\"87213\"></a>Orsaker till konfusion",
				"text": "<table><tbody><tr><td><ul><li>Hjärnskada eller akut hjärnpåverkan</li></ul><ul><li>Läkemedel</li></ul><ul><li>Kroppslig sjukdom</li></ul><ul><li>Psykogena faktorer och miljöfaktorer</li></ul></td></tr></tbody></table>",
				"id": "s7_23"
			},
