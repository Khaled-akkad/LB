ring; uttalat</p>\r\n<p>som vid AN</p>\r\n</td>\r\n<td>\r\n<p>Ej till&auml;mpbart</p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<p><strong>Otillr&auml;ckligt n&auml;rings-</strong></p>\r\n<p><strong>och energiintag</strong></p>\r\n</td>\r\n<td>\r\n<p>Kr&auml;vs f&ouml;r diagnos</p>\r\n</td>\r\n<td>\r\n<p>Kr&auml;vs f&ouml;r diagnos</p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n<td>\r\n<p>Ej till&auml;mpbart</p>\r\n</td>\r\n<td>\r\n<p>Kr&auml;vs f&ouml;r diagnos</p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<p><strong>&Ouml;vervikt</strong></p>\r\n</td>\r\n<td>\r\n<p>F&ouml;rekommer inte</p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n<td>\r\n<p>Kr&auml;vs ej. men</p>\r\n<p>&auml;r vanligt</p>\r\n</td>\r\n<td>\r\n<p>Ej till&auml;mpbart</p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<p><strong>Regelbundna hets-</strong></p>\r\n<p><strong>&auml;tningar (varje vecka)</strong></p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n<td>\r\n<p>Kr&auml;vs f&ouml;r diagnos</p>\r\n</td>\r\n<td>\r\n<p>Kr&auml;vs f&ouml;r diagnos</p>\r\n</td>\r\n<td>\r\n<p>Ej till&auml;mpbart</p>\r\n</td>\r\n<td>\r\n<p>F&ouml;rekommer inte</p>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<p><strong>Regelbundna kompensatoriska</strong></p>\r\n<p><strong>beteenden </strong></p>\r\n<p><strong>(varje vecka)</strong></p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n<td>\r\n<p>Kan f&ouml;rekomma</p>\r\n</td>\r\n<td>\r\n<p>Kr&auml;vs f&ouml;r diagnos</p>\r\n</td>\r\n<td>\r\n<p>F&ouml;rekommer s&auml;llan</p>\r\n</td>\r\n<td>\r\n<p>Ej till&auml;mpbart</p>\r\n</td>\r\n<td>\r\n<p>Regelbunden sj&auml;lvrensning kr&auml;vs,</p>\r\n<p>dock ej kompensatorisk</p>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<p><strong>Un