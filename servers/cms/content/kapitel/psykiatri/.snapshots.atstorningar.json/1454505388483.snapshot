		{
			"id": "f54cdadb-7a4e-4d60-88d1-aa7f7cb48b5f",
			"name": "f54cdadb-7a4e-4d60-88d1-aa7f7cb48b5f",
			"type": "figure",
			"content": {
				"number": "1",
				"maxwidth": "",
				"image": "/images/s4_psy_atstorningar_2013fm10_images/figur1.png",
				"title": "",
				"text": "<a class=\"crossref\" id=\"65021\"></a>Ätstörningar – riskfaktorer och multifaktoriell orsaksmodell för ätstörningar",
				"alt": "",
				"id": "s4_8"
			},
			"settings": {
				"preprocessors": {
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
