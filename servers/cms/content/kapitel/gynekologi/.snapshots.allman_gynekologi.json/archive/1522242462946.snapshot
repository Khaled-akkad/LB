				"id": "i3_38",
				"text": "<table>\r\n<tbody>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\"><strong>Hormonprov</strong></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\"><strong>PCOS</strong></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\"><strong>Hypotalam</strong></div>\r\n<div class=\"tableLine\"><strong>amenorr&eacute;<span class=\"tab\"></span></strong></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\"><strong>Hyperprolaktinemi<span class=\"tab\"></span></strong></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Prematur </div>\r\n<div class=\"tableLine\">ovariell </div>\r\n<div class=\"tableLine\">insufficiens</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">S-FSH</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\"><span class=\"tab\"></span>Normalt/l&aring;gt<span class=\"tab\"></span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt/l&aring;gt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">H&ouml;gt</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">S-LH</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\"><span class=\"tab\"></span>Normalt/h&ouml;gt<span class=\"tab\"></span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt/l&aring;gt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt/h&ouml;gt</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">S-Prolaktin<span class=\"tab\"></span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt/i &ouml;verkant<span class=\"tab\"></span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">H&ouml;gt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">S-TSH</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\"><span class=\"tab\"></span>Normalt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt/h&ouml;gt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">S-SHBG<span class=\"tab\"></span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">L&aring;gt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\"><span>S-Testosteron</span><span class=\"tab\"></span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt/i &ouml;verkant<span class=\"tab\"></span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Normalt</div>\r\n</td>\r\n</tr>\r\n</tbody>\r\n</table>"
