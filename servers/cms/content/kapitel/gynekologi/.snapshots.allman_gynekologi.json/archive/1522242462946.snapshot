		{
			"id": "94adef66-00ab-4482-9f5e-f8c6fd1b537e",
			"name": "94adef66-00ab-4482-9f5e-f8c6fd1b537e",
			"type": "text",
			"content": "<p><span> </span></p>\n<h4 id=\"i3_22\"><em><a class=\"index\" id=\"1025900\" data-text=\"anovulatoriska bl&#xF6;dningar;bl&#xF6;dningar:anovulatoriska\"></a>Anovulatoriska bl&ouml;dningar</em></h4>",
			"settings": {
				"preprocessors": {
					"idinjection.js": "false",
					"fixlinkstoself.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
