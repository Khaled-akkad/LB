\n<td>\r\n<div class=\"tableLine\">Ej fastst&auml;lld</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Ej fastst&auml;llt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">5&ndash;10 dagar</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">Lakosamid</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">200&ndash;400</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Ej aktuell</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Ej fastst&auml;llt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">3 dagar</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">Lamotrigin</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">100&ndash;600<sup>c</sup></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">2&ndash;10<sup>c<a class=\"tableFootnoteLink\" href=\"#fnote_33\"><sup></sup></a></sup></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Ej fastst&auml;llt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">ca 5 dagar</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">Levetiracetam</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">1&nbsp;000&ndash;<span style=\"color: #000000;\">3 000</span></div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">5&ndash;30 (6&zwnj;0)</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Ej fastst&auml;llt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">2 dagar</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">Nitrazepam</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">5</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Ej fastst&auml;lld</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Ej fastst&auml;llt</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">1&ndash;2 dagar</div>\r\n</td>\r\n</tr>\r\n<tr>\r\n<td>\r\n<div class=\"tableLine\">Oxkarbazepin</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">600&ndash;2&nbsp;400</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">30&ndash;46 (vanligen 30)</div>\r\n</td>\r\n<td>\r\n<div class=\"tableLine\">Ej fastst&auml;llt</div>\r\n</