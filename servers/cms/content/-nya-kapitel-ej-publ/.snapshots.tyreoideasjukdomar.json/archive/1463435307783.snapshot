		{
			"id": "11529aeb-7e59-4f8d-869f-af35943e0baf",
			"name": "11529aeb-7e59-4f8d-869f-af35943e0baf",
			"type": "therapy",
			"content": {
				"number": "4",
				"title": "<a class=\"index\" id=\"51263\"></a>Atoxisk struma",
				"id": "k3_36",
				"text": "<table><tbody><tr><td><div class=\"tableLine\"><strong><span style=\"font-weight: bold;\">Autoimmun tyreoidit</span></strong></div></td><td><div class=\"tableLine\">Tyroxin</div></td></tr><tr><td><div class=\"tableLine\"><strong>Ej autoimmun genes</strong></div></td><td><div class=\"tableLine\"></div></td></tr><tr><td><div class=\"tableLine\">TSH-stegring</div></td><td><div class=\"tableLine\">Tyroxin</div></td></tr><tr><td><div class=\"tableLine\">Trycksymtom</div></td><td><div class=\"tableLine\">Tyroxin försöksvis</div></td></tr><tr><td><div class=\"tableLine\"></div></td><td><div class=\"tableLine\">Operation</div></td></tr><tr><td><div class=\"tableLine\"></div></td><td><div class=\"tableLine\">Radioaktivt jod om </div><div class=\"tableLine\">operation är olämplig</div></td></tr><tr><td><div class=\"tableLine\">Asymtomatisk</div></td><td><div class=\"tableLine\">Exspektans</div></td></tr><tr><td><div class=\"tableLine\"><strong>Autonom funktion</strong></div></td><td><div class=\"tableLine\">Kontraindikation för </div><div class=\"tableLine\">tyroxinterapi</div></td></tr><tr><td><div class=\"tableLine\"><strong>Tumörmisstanke</strong></div></td><td><div class=\"tableLine\">Operation</div></td></tr></tbody></table>"
			},
			"settings": {
				"preprocessors": {
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
					"genericas.js": "false",
