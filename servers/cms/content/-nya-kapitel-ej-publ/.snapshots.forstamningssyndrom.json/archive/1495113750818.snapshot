		{
			"id": "7abdd15a-cfaa-4710-bb43-c543574ab6b5",
			"name": "7abdd15a-cfaa-4710-bb43-c543574ab6b5",
			"type": "figure",
			"content": {
				"number": "4",
				"maxwidth": "",
				"image": "/images/kapitel/psykiatri/forstamningssyndrom/figur4.png",
				"title": "",
				"text": "<p>\"Graviditetssymtom\" som kan f&ouml;rv&auml;xlas med depressionssymtom</p>",
				"alt": "",
				"id": "s3_65"
			},
			"settings": {
				"preprocessors": {
					"idinjection.js": "false"
				},
				"postprocessors": {
					"boxlinks.js": "false",
